{
    "version": 2,
    "builds": [
        {
            "src": "server/start.js",
            "use": "@vercel/node"
        },
        {
            "src": "app/dist/app/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*",
            "dest": "/server/start.js"
        }
    ],
    "env": {
        "NEW_RELIC_HOME": "/vercel/path/to/newrelic"
    }
}