
{
    "version": 2,
    "builds": [
      {
        "src": "server/start.js",
        "use": "@vercel/node"
      },
      {
        "src": "app/dist/app/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/start.js"
      },
      {
        "src": "/(.*)",
        "dest": "app/dist/app/$1"
      }
    ]
  }





  